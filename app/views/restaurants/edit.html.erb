<%= form_with(model: restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :address %>
  <%= f.label :details %>
  <%= f.text_field :details %>

  <% if !f.object.new_record? %>
    <%= f.label :completed %>
    <%= f.check_box :completed %>
  <% end %>

  <%= f.submit 'Submit' %>
<% end %>
